class Solution(object):
    def maxArea(self, height):
        a = 0
        b = len(height)-1
        lena = 0
        lenb = 0
        stack = [0]
        while a != b:
            ms = min(height[a], height[b])
            diff = b-a
            stack.append(diff*ms)
            if(height[a] >= height[b]):
                b = b-1
            else:
                a = a+1

        stack.sort()

        return max(stack)
